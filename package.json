{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "watch:sass": "node-sass src/sass/main.scss src/css/style.css -w",
    "start": " react-scripts start watch:sass ",
    "prefix:css": "postcss --use autoprefixer -b 'last 11 versions' css/style.concat.css -o css/style.prefix.css",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@react-three/drei": "^7.13.1",
    "@react-three/fiber": "^7.0.10",
    "autoprefixer": "^10.3.7",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "react-particles": "^0.0.3",
    "react-particles-js": "^3.5.3",
    "style-components": "^0.1.0",
    "styled-components": "^5.3.1",
    "three": "^0.133.1"
  }
}
